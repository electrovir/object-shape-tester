    },
    "type": "module",
    "main": "src/index.ts",
    "module": "src/index.ts",
    "types": "src/index.ts",
    "scripts": {
        "build": "virmator frontend build",
        "compile": "virmator compile",
        "docs": "virmator docs",
        "format": "virmator format",
