{
    "name": "object-shape-tester",
    "version": "1.0.0",
    "description": "Test object properties and value types.",
    "keywords": [],
    "homepage": "https://github.com/electrovir/object-shape-tester",
    "bugs": {
        "url": "https://github.com/electrovir/object-shape-tester/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/electrovir/object-shape-tester"
    },
    "license": "(MIT or CC0 1.0)",
    "author": {
        "name": "electrovir",
        "url": "https://github.com/electrovir"
    },
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/types/index.d.ts",
    "scripts": {
        "build": "virmator frontend build",
        "compile": "rm -rf dist && tsc --project tsconfig.json && tsc --project tsconfig.cjs.json",
        "docs": "virmator docs",
        "format": "virmator format",
        "publish": "virmator publish \"npm run compile && npm run test:all\"",
        "start": "npm install && virmator frontend",
        "test": "virmator test-web",
        "test:all": "concurrently --kill-others-on-fail --colors -c auto --names types,tests,spelling,format,docs,deps \"npm run test:types\" \"npm run test:coverage\" \"npm run test:spelling\" \"npm run test:format\" \"npm run test:docs\" \"npm run test:deps\"",
        "test:coverage": "npm run test coverage",
        "test:deps": "virmator deps check",
        "test:docs": "virmator docs check",
        "test:format": "virmator format check",
        "test:spelling": "virmator spellcheck",
        "test:types": "tsc --noEmit"
    },
    "dependencies": {
        "@augment-vir/common": "^21.3.6",
        "type-fest": "^4.6.0"
    },
    "devDependencies": {
        "@augment-vir/browser": "^21.3.6",
        "@augment-vir/browser-testing": "^21.3.6",
        "@augment-vir/node-js": "^21.3.6",
        "@open-wc/testing": "^3.2.0",
        "@types/chai": "^4.3.9",
        "@types/mocha": "^10.0.3",
        "@web/dev-server-esbuild": "^0.4.3",
        "@web/test-runner": "^0.17.2",
        "@web/test-runner-commands": "^0.8.2",
        "@web/test-runner-playwright": "^0.10.2",
        "@web/test-runner-visual-regression": "^0.8.2",
        "concurrently": "^8.2.2",
        "cspell": "^7.3.8",
        "dependency-cruiser": "^15.0.0",
        "esbuild": "^0.19.5",
        "istanbul-smart-text-reporter": "^1.1.2",
        "markdown-code-example-inserter": "^0.3.2",
        "npm-check-updates": "~16.12.3",
        "prettier": "^3.0.3",
        "prettier-plugin-interpolated-html-tags": "^1.0.2",
        "prettier-plugin-jsdoc": "^1.1.1",
        "prettier-plugin-multiline-arrays": "^3.0.0",
        "prettier-plugin-organize-imports": "^3.2.3",
        "prettier-plugin-packagejson": "^2.4.6",
        "prettier-plugin-sort-json": "^3.1.0",
        "prettier-plugin-toml": "^1.0.0",
        "run-time-assertions": "^0.1.3",
        "typedoc": "^0.25.2",
        "typescript": "^5.2.2",
        "virmator": "^10.0.0",
        "vite": "^4.5.0",
        "vite-tsconfig-paths": "^4.2.1"
    }
}
